<div class="col s6 form">
    <%= form_for @car do |f| %>
    <p>
        <%= f.label :make %> <br />
        <%= f.text_field :make, class: "form" %>
    </p>
    
    <p>
        <%= f.label :model %> <br />
        <%= f.text_field :name, class: "form" %>
    </p>
    
    <p>
        <%= f.label :price %> <br />
        <%= f.text_field :price, class: "form" %>
    </p>
    
    <p>
        <%= f.label :description %> <br />
        <%= f.text_area :description, class: "form" %>
    </p>
    
    <p>
        <%= f.label :picture %> <br />
        <%= f.text_area :picture, class: "form" %>
    </p>
    
    <p>
        <%= f.submit %>
    </p>

<% end %>
</div>